{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft.AspNetCore": "Trace"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5000"
      },
      "Https": {
        "Url": "https://*:5001",
        "Certificate": {
          "Path": "obj/cert.crt",
          "KeyPath": "obj/key.key"
        }
      }
    }
  },
  "ConnectionOptions": {
    "IsProxyUsed": false
  },
  "ConnectionStrings": {
    "SecureDb": "FileName=obj/secure.db"
  },
  "MasterKeyServiceOptions": {
    "MaxAttemptCounts": 2,
    "BlockTimeout": "00:00:10"
  },
  "UserOptionsPath": "obj/usersettings.json"
}
