{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft.AspNetCore": "Trace"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5000"
      }
    }
  },
  "ConnectionOptions": {
    "IsProxyUsed": false
  },
  "ConnectionStrings": {
    "SecureDb": "FileName=obj/database.db"
  },
  "KeyServiceOptions": {
    "MaxAttemptCounts": 2,
    "BlockTimeout": "00:00:10"
  },
  "UserOptionsPath": "obj/usersettings.json"
}
