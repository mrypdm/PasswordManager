{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SessionOptions": {
    "IsProxyUsed": false
  },
  "ConnectionStrings": {
    "SecureDb": "FileName=/etc/password-manager/database.db"
  },
  "MasterKeyServiceOptions": {
    "MaxAttemptCounts": 3,
    "BlockTimeout": "00:01:00"
  },
  "UserOptionsPath": "/etc/password-manager/usersettings.json"
}
